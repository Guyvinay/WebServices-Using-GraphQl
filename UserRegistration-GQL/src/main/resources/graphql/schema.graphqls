type Query{
     allUsers:[Users]
     getUserById(id:String):Users
}

type Mutation {
     register(user:UsersReq):UsersReq
}

type Users {
     user_id:ID!
     name:String
     mobile:String
     role:String
     email:String
     password:String
}

type UsersReq {
     name:String
     mobile:String
     email:String
     role:String
     password:String
}