type Query{
     allUsers:[Users]
     getUserById(id:String):Users
}

type Mutation {
     createUser(user:UsersReq):UsersReq
}

type Users {
     user_id:ID!
     name:String
     mobile:Long
     role:String
     email:String
     password:String
     address:String
}

type UsersReq {
     name:String
     mobile:Long
     email:String
     password:String
     address:String
}